{
    "name": "Social Links",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Build to get Job!",
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }],
    "browser_action": {
        "default_title": "Social Links",
        "default_popup": "popup.html",
        "default_icon": {
            "19": "images/icon.png",
            "128": "images/icon_128.png"
          }
    }
}